    local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
    local Window = Library.CreateLib(" GPO Fruit Esp ", "DarkTheme")


    --main
    local Main = Window:NewTab("Player")
    local PlayerSection = Main:NewSection("Player")

    --main
    local Main = Window:NewTab("Main")
    local MainSection = Main:NewSection("Main")


    --Player
    PlayerSection:NewSlider("WalkSpeed", "Makes you walk faster ig", 500, 16, function(v) -- 500 (MaxValue) | 0 (MinValue)
        game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = v
    end)


    --Main
    MainSection:NewToggle("Fruit  Esp", "for afk need to be op", function(v)
        getgenv().autospin = v
        while wait() do
            if not getgenv().autospin then return 
                for a,b in pairs(game:GetService("Workspace"):GetDescendants())do if b.Name=="preHandle"then local c=Instance.new("BillboardGui",b)c.Name="ElectricPPPGUI"c.Size=UDim2.new(50,0,50,0)c.AlwaysOnTop=true;local d=Instance.new("Frame",c)d.Size=UDim2.new(1,0,1,0)d.BackgroundTransparency=0.80;d.BorderSizePixel=0;d.BackgroundColor3=Color3.fromRGB(255,125,38)local e=Instance.new('TextLabel',d)e.Size=UDim2.new(2,0,2,0)e.BorderSizePixel=0;e.TextSize=20;e.Text="Fruit"e.BackgroundTransparency=1;e.TextColor=Color3.fromRGB(255,255,255)end end
            end
         end
    end)

